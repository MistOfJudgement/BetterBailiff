{
  "name": "betterbailiff",
  "version": "1.0.0",
  "description": "Rework of the bailiff bot",
  "main": "dist/src/bot.js",
  "scripts": {
    "start": "npm run build && node ./dist/src/bot.js",
    "build": "tsc --build",
    "tslint": "tslint src/**/*.ts",
    "tslint:fix": "npm run tslint -- --fix",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "format": "prettier --config .prettierrc src/**/*.ts --write"
  },
  "author": "Tushar Rangaswamy",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.8.1",
    "discord-api-types": "^0.37.84",
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "eslint": "^9.3.0"
  }
}
